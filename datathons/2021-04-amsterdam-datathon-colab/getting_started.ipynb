    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "xYID1Df_YZ0I"
      },
      "source": [
        "lactate_ids = dictionary[dictionary['item'].str.contains(r'lactaat|lactate', regex=True, case=False)]\n",
