    "outputId": "f2ffce57-b38f-437b-cc93-ee0977ba31ed"
   },
   "outputs": [],
   "source": [
    "%%bigquery\n",
    "SELECT * FROM admissions\n",
    "LIMIT 100"
   ]
  },
